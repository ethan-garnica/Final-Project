<!DOCTYPE html>
<html>
<head>
   <%- include ("partials/header.ejs")%>
</head>
<body>
<!--     <h1>hello</h1> -->
    <main>
        <header class="p-5 bg-danger text-center">
          <h1>Company Results</h1>
          <%- include ("partials/menu.ejs")%>
        </header>
			
        <form action = "/company" method ="GET">      
        <br>               
        <div id="companies">           
            
          <% for(let i = 0; i <compNum;i++) {%>
                    
          	<div class="groupResults">                     
              <label class="radio-inline">
								<input type = "radio" name="company" value=<%=compId[i]%>> <%=nameList[i]%> <img src = "
									<%=imageNameList[i]%>" width="100">
							</label>                                   
                   
            </div>
                    
          <%}%>                   
            
        </div>
        <button type="submit" class="m-3 btn btn-dark">Submit</button>
        </form>            
                    
    </main>
</body>
</html>